variable "region" {
  description = "The AWS region to deploy to"
  default     = "us-west-1"
  type        = string
}

variable "bucket_name" {
  description = "The name of the S3 bucket"
  default     = "iowa-data"
  type        = string
}

variable "environment" {
  description = "The environment to deploy to"
  default     = "dev"
  type        = string
}
